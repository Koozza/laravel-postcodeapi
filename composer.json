{
  "name": "nickurt/laravel-postcodeapi",
  "description": "Universal PostcodeApi for Laravel 6.x/7.x/8.x",
  "keywords": [
    "postcodeapi",
    "postcode",
    "api",
    "universal",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0|^7.4",
    "laravel/framework": "^6.0|^7.0|^8.0",
    "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.15|^8.4|^9.3.3",
    "orchestra/testbench": "^4.0|^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "nickurt\\PostcodeApi\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "nickurt\\PostcodeApi\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "nickurt\\PostcodeApi\\ServiceProvider"
      ],
      "aliases": {
        "PostcodeApi": "nickurt\\PostcodeApi\\Facade"
      }
    }
  }
}
